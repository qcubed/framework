{
  "name": "qcubed/qcubed",
  "description": "QCubed is a PHP framework for rapid web development. Building on top of jQuery, with OOP at its core, QCubed provides a powerful solution for building database-driven Web 2.0 applications.",
  "keywords": ["php", "framework", "jquery"],
  "homepage": "http://qcu.be/",
  "type": "qcubed-framework",
  "license": "MIT",
  "support": {
      "issues": "https://github.com/qcubed/qcubed/issues",
      "forum": "http://qcu.be/forum",
      "wiki": "https://github.com/qcubed/qcubed/wiki",
      "source": "https://github.com/qcubed/qcubed",
      "irc": "irc://irc.freenode.net/qcubed"
  },
  "require": {
      "php": ">=5.4.0",
      "qcubed/composer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "codeclimate/php-test-reporter": "^0.3.2"
  },
  "suggest": {
    "ezyang/htmlpurifier": "dev-master",
    "fortawesome/font-awesome": "^4.6"
  },
  "autoload": {
    "psr-4": {
      "QCubed\\Devtools\\": "includes/_devtools/"
    }
  },
  "extra": {
    "writePermission": [
        "project/generated",
        "project/generated/connector_base",
        "project/generated/drafts",
        "project/generated/drafts/panels",
        "project/generated/form_base",
        "project/generated/model_base",
        "project/includes/connector",
        "project/includes/model",
        "project/tmp",
        "project/forms",
        "project/includes/dialog",
        "project/includes/panel",
        "project/includes/configuration/codegen_options.json"
    ]
  }
}
